@page "/roomchartlist/{Location}/{RoomId}/{RoomName}"

<RoomChartView RoomId="@RoomId" RoomName="@RoomName" Location="@Location"></RoomChartView>

@code {
    [Parameter]
    public string RoomId { get; set; } = string.Empty;

    [Parameter]
    public string RoomName { get; set; } = string.Empty;

    [Parameter]
    public string Location { get; set; } = string.Empty;
}