@page "/roomchartlist/{Location}/{RoomId}/{RoomName}"

<AuthorizeView Policy="administrator">
    <Authorized>
        <RoomChartListView RoomId="@RoomId" RoomName="@RoomName" Location="@Location"></RoomChartListView>
    </Authorized>
</AuthorizeView>

@code {
    [Parameter]
    public string RoomId { get; set; } = string.Empty;

    [Parameter]
    public string RoomName { get; set; } = string.Empty;

    [Parameter]
    public string Location { get; set; } = string.Empty;
}