@if ((this.Models != null) && (this.Models.Any() == true))
{
    foreach (RoomChartModel model in this.Models)
    {
        <RoomChart Labels="@model.Labels" TemperatureOUT="@model.TemperaturesOUT" TemperatureIN="@model.TemperaturesIN" Humidities="model.Humidities" CurrentDay="@model.Day"></RoomChart>
    }
}

@code
{
    [Parameter]
    public IEnumerable<RoomChartModel>? Models { get; set; }

}
